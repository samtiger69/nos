@model List<nosee.Models.Item>
@{
    ViewBag.Title = "About";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>


<table class="table table-hover">
    <tr>
        <td>
            <b>ID</b>
        </td>
        <td>
            <b>Name</b>
        </td>
    </tr>
    @foreach (var v in Model)
    {
        <tr>
            <td>
                @v.Id
            </td>
            <td>
                @v.Name
            </td>
        </tr>
    }
</table>

<p>Use this area to provide additional information.</p>
